{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".test.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@context/(.*)$": "<rootDir>/../src/context/$1",
    "^@test/(.*)$": "<rootDir>/../test/$1"
  }
}
